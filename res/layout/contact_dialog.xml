<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="#ffffff">
    
    <TextView 
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="联系TA"
        android:textSize="25dp"
        android:textColor="#fd6c02"
        android:layout_marginTop="7dp"
        android:layout_marginBottom="5dp"
        android:id="@+id/tv_sure_dialog_title"
        />
        <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="#fd6c02"
        ></LinearLayout>
    <Button 
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:id="@+id/bt_contact_dialog_tel"
        android:text="拨打电话"
        android:textColor="#636363"
        android:layout_gravity="center"
        android:background="#00ffffff"
        android:textSize="15dp"/>
    <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:background="#fd6c02"
        android:orientation="horizontal"
        android:id="@+id/l_contact_dialog_tel"
        ></LinearLayout>
    
    <Button 
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:id="@+id/bt_contact_dialog_mes"
        android:text="发送短信"
        android:textColor="#636363"
        android:layout_gravity="center"
        android:background="#00ffffff"
        android:textSize="15dp"/>
    <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:background="#fd6c02"
        android:orientation="horizontal"
        android:id="@+id/l_contact_dialog_qq"
        ></LinearLayout>
    <Button 
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:id="@+id/bt_contact_dialog_qq"
        android:text="复制QQ号"
        android:textColor="#636363"
        android:layout_gravity="center"
        android:background="#00ffffff"
        android:textSize="15dp"/>

    

</LinearLayout>
